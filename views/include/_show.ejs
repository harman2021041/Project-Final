<% for(var i = 0; i < users.length; i++) { %>
<tr >
    <td><%= i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>" class="btn bggg update">
            <span class="text-light"><i class="fas fa-pencil-alt">update</i></span>
        </a>
        <a class="btn bggg delete" data-id=<%= users[i]._id%> >
            <span class="text-light"><i class="fas fa-times">delete</i></span>
        </a>
    </td>
</tr>
<% } %>
